  "integrationNotes": {
    "fishbowlMatching": {
      "description": "How Fishbowl customers match to Copper companies",
      "mapping": {
        "fishbowl_customers.accountId": "copper_companies['Account ID cf_713477']",
        "note": "Both are string representations of Copper's internal ID"
      }
    },
    "orderDataSource": {
      "description": "Order data comes from sales_order_history subcollection",
      "path": "fishbowl_customers/{customerId}/sales_order_history",
      "dateRange": "2023 to present"
    },
    "activeCustomerLogic": {
      "description": "Active Customer field is a boolean",
      "values": {
        "true": "Active customer",
        "false": "Inactive customer",
        "unchecked": "Legacy string value (deprecated)"
      },
      "query": "Use .where('Active Customer cf_712751', '==', true) to get active customers"
    }
